  // Old vanilla reworked ruins with a pretty rare default spawn chance
  {
    "op": "add",
    "path": "/structures/-",
    "value": {
                  "code": "brsurfaceruins-small",
                  "chance": 0.008, // # default = 0.008 defines how often this structure pool will spawn increase to let is spawn more often and decrease to make it more rare.
